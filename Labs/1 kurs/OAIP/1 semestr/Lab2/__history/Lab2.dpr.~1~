program Lab2;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

var
 M, I, N: integer;
begin
 writeln('¬ведите число M');
 readln(M);
 i:= 2;
 while (M>1) do
 begin
 if (M mod I <> 0) then
 I:= I + 1
 else
 begin
 Readln(I);
 M:= M mod I;
 end;
 end;
 readln;

end.
