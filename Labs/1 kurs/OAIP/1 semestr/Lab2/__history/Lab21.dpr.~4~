  for P := 2 to N do
    B[P]:=true;
    P:=2;

    while (P*P <= N) do
    begin
      i:=P*P;
      if B[P] then
      begin
       while i <= N do
       begin
        B[i]:=false;
        i:=i+P;

        n:= n div p;
       end;
       writeln(P);
      end;
      P:=P+1;
    end;



readln(N);
end.
