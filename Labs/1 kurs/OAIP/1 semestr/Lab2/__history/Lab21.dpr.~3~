program Lab21;

{$APPTYPE CONSOLE}

{$R *.res}
uses
  System.SysUtils;
var
N,i,P: integer;
 B : array[1..1000] of boolean;
begin
write ('N > ');
read(N);
  for P := 2 to N do
    B[P]:=true;
    P:=2;
    while (P*P <= N) do
    begin
      i:=P*P;
      if B[P] then
      while i <= N do
      begin
        B[i]:=false;
        i:=i+P;
      end;
      P:=P+1;
    end;
    write('Простые числа: ');
    for P := 2 to n do
      if B[P]=true then
      write(P, ' ');
      n:= n div p;
readln(N);
end.
