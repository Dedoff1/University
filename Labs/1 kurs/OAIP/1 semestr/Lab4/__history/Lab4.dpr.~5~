program Lab4;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

const
N=5;
var
X:array[1..N] of real;
i,v,r,L:integer;
m:real;
begin
 for i:=1 to n do
 begin
   write('¬ведите элемент массива', 'X[',i,']= ');
   Readln(X[i]);
 end;
 for i:= 1 to n do
 begin
   M := X[i];
   for v := i+1 to n do
    if X[i] = M then
     L := L + 1;
   if L = 5 then
     writeln('1');
   if L = 4 then
     writeln('2');

    if (L = 3)  and (r = 1) then
     w := 2;
     if (L = 3)  and (r = 2) then
      writeln('3');
     else
      r:=r+1;
    if L = 2 and (r = 1) and (w = 1) then
     if r = 2 then
      if L[r-1]=2 then
      begin
       writeln('5');
       break;
      end
      else
      begin
       writeln('3');
       break;
      end
      else
       r:=r+1
     else
     begin
      writeln('7');
      break;
     end;
 end;
 readln(I);
end.
