program Lab4;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

const
N=5;
var
X:array[1..N] of real;
L:array[1..2] of integer;
i,v,r:integer;
m:real;
begin
 for i:=1 to n do
 begin
   write('¬ведите элемент массива', 'X[',i,']= ');
   Readln(X[i]);
 end;
 for i:= 1 to n do
 begin
   M := X[i];
   for v := i+1 to n do
    if X[i] = M then
     L[r] := L[r]+1;
    if L[r] = 5 then
     writeln('1');
    if L[r] = 4 then
    writeln('2');
    if L[r] = 3 then
     if r = 2 then
      writeln('3')
     else
      r:=r+1;
    if L[r] = 2 then
     if r = 2 then
      if L[r-1]=2 then
       writeln('5')
      else
       writeln('3')
      else
       r:=r+1
     else
    writeln('7');
 end;
 readln(I);
end.
